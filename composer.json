{
    "name": "enabel/symfony",
    "description": "Another Symfony application made by Enabel IT Team.",
    "type": "project",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "StÃ©phane DELBEKE",
            "email": "stephane.delbeke@enabel.be"
        },
        {
            "name": "Damien Lagae",
            "email": "damien.lagae@enabel.be"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-mbstring": "*",
        "composer/package-versions-deprecated": "^1.11",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.1",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.7",
        "easycorp/easyadmin-bundle": "^3.1",
        "knpuniversity/oauth2-client-bundle": "^2.5",
        "mez/grumphp-tasks": "^1.3",
        "php-http/guzzle7-adapter": "^0.1.0",
        "php-translation/loco-adapter": "^0.11.0",
        "php-translation/symfony-bundle": "^0.12.1",
        "phpdocumentor/reflection-docblock": "^5.2",
        "predis/predis": "^1.1",
        "sensio/framework-extra-bundle": "^5.2",
        "snc/redis-bundle": "^3.2",
        "stof/doctrine-extensions-bundle": "^1.5",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "5.2.*",
        "symfony/console": "5.2.*",
        "symfony/dotenv": "5.2.*",
        "symfony/expression-language": "5.2.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "5.2.*",
        "symfony/framework-bundle": "5.2.*",
        "symfony/http-client": "5.2.*",
        "symfony/intl": "5.2.*",
        "symfony/mailer": "5.2.*",
        "symfony/mime": "5.2.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/notifier": "5.2.*",
        "symfony/process": "5.2.*",
        "symfony/property-access": "5.2.*",
        "symfony/property-info": "5.2.*",
        "symfony/requirements-checker": "^2.0",
        "symfony/security-bundle": "5.2.*",
        "symfony/serializer": "5.2.*",
        "symfony/string": "5.2.*",
        "symfony/translation": "5.2.*",
        "symfony/twig-bundle": "^5.2",
        "symfony/validator": "5.2.*",
        "symfony/web-link": "5.2.*",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/yaml": "5.2.*",
        "thenetworg/oauth2-azure": "^2.0",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "escapestudios/symfony2-coding-standard": "^3.11",
        "friendsoftwig/twigcs": "^3.2",
        "phpmd/phpmd": "^2.9",
        "phpro/grumphp": "^0.22",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "^0.12.48",
        "phpstan/phpstan-doctrine": "^0.12.20",
        "phpstan/phpstan-mockery": "^0.12.7",
        "phpstan/phpstan-phpunit": "^0.12.16",
        "phpstan/phpstan-symfony": "^0.12.8",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/browser-kit": "^5.2",
        "symfony/css-selector": "^5.2",
        "symfony/debug-bundle": "^5.2",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^5.2",
        "symfony/stopwatch": "^5.2",
        "symfony/var-dumper": "^5.2",
        "symfony/web-profiler-bundle": "^5.2",
        "vimeo/psalm": "^4.6",
        "zenstruck/foundry": "^1.6"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "requirements-checker": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "5.2.*"
        }
    }
}
